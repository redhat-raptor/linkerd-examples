{
  "containerDefinitions": [
    {
      "volumesFrom": [],
      "memory": 1024,
      "portMappings": [
        {
          "hostPort": 4140,
          "containerPort": 4140,
          "protocol": "tcp"
        },
        {
          "hostPort": 9990,
          "containerPort": 9990,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "mountPoints": [
        {
          "containerPath": "/etc/linkerd",
          "sourceVolume": "linkerd-config",
          "readOnly": true
        }
      ],
      "name": "linkerd",
      "environment": [],
      "image": "docker.io/buoyantio/linkerd:1.1.2",
      "command": [
        "/etc/linkerd/linkerd.yaml"
      ],
      "cpu": 0,
      "memory": 1024
    }
  ],
  "volumes": [
    {
      "host": {
        "sourcePath": "/home/ec2-user/linkerd-config"
      },
      "name": "linkerd-config"
    }
  ],
  "family": "linkerd"
}
